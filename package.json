    "gulp": "^4.0.2",
    "gulp-html-minifier": "^0.1.8",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^9.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-svg-sprite": "^2.0.3",
    "gulp-version-number": "^0.2.4",
    "gulp-webp": "^5.0.0",
    "postcss-csso": "^6.0.1",
    "sass": "^1.69.5",
    "webpack": "^5.89.0",
    "webpack-stream": "^7.0.0",
